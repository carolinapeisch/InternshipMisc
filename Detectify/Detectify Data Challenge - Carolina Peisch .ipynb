   "source": [
    "from threading import Thread, Condition\n",
    "import time\n",
    "import random\n",
    "\n",
    "#setting buffer to empty list\n",
    "buffer = []\n",
    "\n",
    "#setting buffer_ceiling to static value of 50 to begin\n",
    "buffer_ceiling = 50\n",
    "\n",
    "#Condition object \n",
